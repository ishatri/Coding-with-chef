#include <iostream>
#include <bits/stdc++.h>
#include <cmath>
using namespace std;
int main()
{
		#ifndef ONLINE_JUDGE
	freopen("input.txt","r",stdin);
	freopen("output.txt","w" ,stdout);
	#endif
	int t;
	cin>>t;
	while(t--)
	{
		int n;
		cin>>n;
		long double x;
		x= n*0.143;
		double c = pow(x,n);
		int r= round(c);
		cout<<r<<endl;
	}
	return 0;
}
